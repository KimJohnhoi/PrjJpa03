{{>layouts/header}}
  
 <!-- begin content -->
<form class="container" action="/article/Write" method="post">
  <div class="mb-3">
    <label for="title" class="form-label">제목</label>
    <input type="text" class="form-control" name="title" id="title" >
  </div>
  <div class="mb-3">
    <label for="content" class="form-label">내용</label>
    <textarea class="form-control" rows="4" name="content" id="content"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
  <a href="/article/List" class="btn btn-outline-primary" role="button">List</a>
  <a href="/" class="btn btn-outline-primary" role="button">Home</a>
</form>
 <!-- end of content -->

