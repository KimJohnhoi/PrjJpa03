{{>layouts/header}}
  
 <!-- begin content -->
<div class="mb-3 row">
  <label for="staticId" class="col-sm-2 col-form-label">글번호</label>
  <div class="col-sm-10">
    <input type="text" readonly class="form-control-plaintext" id="staticId" value="{{ article.id }}">
  </div>
</div>
<div class="mb-3 row">
  <label for="staticTitle" class="col-sm-2 col-form-label">글제목</label>
  <div class="col-sm-10">
    <input type="text" readonly class="form-control-plaintext" id="staticTitle" value="{{ article.title }}">
  </div>
</div>
<div class="mb-3 row">
  <label for="staticContent" class="col-sm-2 col-form-label">내용</label>
  <div class="col-sm-10">
    <textarea readonly class="form-control" rows="4" name="content" id="staticContent">{{ article.content }}</textarea>
  </div>
</div>


<div class="mx-auto" style="width:90%">
	<a href="/article/WriteForm" class="btn btn-primary">새 글 쓰기</a>
	<a href="/article/List" class="btn btn-primary">목록</a>
	<a href="/article/{{ id }}/UpdateForm" class="btn btn-primary">수정</a>
	<a href="/article/{{ id }}/Delete" class="btn btn-primary">삭제</a>
	<a href="/" class="btn btn-primary">Home</a>
</div>
 <!-- end of content -->

{{>layouts/footer}}